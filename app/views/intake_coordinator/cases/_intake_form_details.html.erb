<script>
  $(document).ready(function() {
    setTimeout(function() {
      $('#collexp-menu_1 > li > div.expanded + ul').slideToggle('medium');
      $('#collexp-menu_1 > li > div').click(function() {
        $('#collexp-menu_1 > li > div.expanded').not(this).toggleClass('expanded').toggleClass('collapsed').parent().find('> ul').slideToggle('medium');
        $(this).toggleClass('expanded').toggleClass('collapsed').parent().find('> ul').slideToggle('medium');
      });
    }, 250);
  });

  function assign_case_counselor(ids) {
    $.ajax({
      url: "/intake_coordinator/dashboards/assign_case_counselor",
      data: {
        ids: ids
      },
      method: 'POST'
    });
  }

  $(document).ready(function() {
    $('#start_at, #end_at').datetimepicker({
      timepicker: true,
      format: 'Y-m-d H:i'
    });
  });
</script>