<table class="table-borLess" cellpadding="0" cellspacing="0" width="100%" border="0">
  <tr>
    <td colspan="5">
      <ul class="list-breadCurm">
        <li class="active"><a href="#" title="Case Number">Lock</a></li>
        <li>Delete</li>
      </ul>
    </td>
  </tr>
  <% @documents.each do |document| %>
    <tr>
      <td>
        <% if document.doc_type == "progress_note" %>
          <%= link_to document.doc_type, edit_counselor_progress_note_path(document.progress_note_id),:remote => true %>
        <% elsif document.doc_type == "intake_evalution" %>
          <%= link_to document.doc_type,edit_initial_evalution_path(current_user.role,document.intake_evalution_id) %>
        <% elsif document.doc_type == "discharge_summary" %>
          <%= link_to document.doc_type,edit_discharge_summary_path(current_user.role,document.discharge_summary_id) %>
        <% elsif document.doc_type == "adolesment_intake" %>
          <%= link_to document.doc_type,edit_adolesment_intake_path(current_user.role,document.adolesment_intake_id) %>
        <% elsif document.doc_type == "intake_form"   %>
          <%= link_to document.intake_form.intake_type,intake_form_details_path(:role => current_user.role,:id => document.intake_form_id) %>
        <% elsif document.doc_type == "reduced_fee" %>
          reduced fee
        <% else %>
          <%= link_to document.document_content_type,document_path(current_user.role,document) %>
        <% end %>
      </td>
      <td><%= document.case ? document.case.number : '' %></td>
      <td>edited: <%= document.updated_at.strftime("%B %d, %Y").upcase %></td>
      <td>created: <%= document.created_at.strftime("%B %d, %Y").upcase %></td>
    </tr>
  <% end %>
</table>