<div class="clientPortal">
  <ul class="list-clientinfo">
    <li>
      <div class="box">
        <h4>Account Information</h4>
        <div class="columnRight-client">
          <span><%= @client.first_name %> <%= @client.last_name %></span>
          <p><%= @client.address %><br>
            <%= @client.city %>,<%= @client.state %> <%= @client.zipcode %></p>
          <p><%= @client.email %></p>
          <p>HOME      <%= number_to_phone(@client.home) %></p>
          <p>MOBILE  <%= number_to_phone(@client.mobile) %></p>
          <p>OFFICE    <%= number_to_phone(@client.work) %></p>
          <div style="float:right;"><%= link_to 'Edit',edit_setting_path(:role => current_user.role,:id => current_user.id) %></div>
        </div>
        <div class="clearFix"></div>
      </div>
    </li>
    <li>
      <div class="box">
        <div class="valignDiv">
          <div>
            <h2>Intake Forms</h2>
            <%= link_to "INTAKE","/client/intakes" %>
          </div>
        </div>
      </div>
    </li>
    <li>
      <div class="box">
        <div class="valignDiv">
          <div><h2 style="font-size: 15px;"><%#= @appointment ? ('appointment with <br />'+@appointment.counselor.first_name+' on <br />'+(@appointment.start_at.blank? ? '' : @appointment.start_at.strftime("%b %dth %I:%m %p")).html_safe) : "No Appointment" %></h2></div>
        </div>
      </div>
    </li>
  </ul>
</div>
<div class="inTake">
  <ul class="list-intake">
    <% for intake in @intake_forms %>
      <li>
        <div class="box">
          <div class="valignDiv">
            <div><h2><%= intake.intake_type %></h2><em><%= intake.intake_status %></em></div>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
</div>
